<div class="container">
  <div class="row">
    <h1>Booking show</h1>
    <p> Name of the storage space to be displayed</p>
    <p>start_date: <%= @booking.start_date %></p>
    <p>end_date <%= @booking.end_date %></p>
  </div>
  <div>
    <h1>Reviews of that storage space</h1>
      <% @booking.reviews.each do |review| %>
      <p>Description: <%= review.description %></p>
      <% end %>
  </div>
</div>

<%= link_to "back" %>
