<%= content_for(:title) do %>
All Bookings
<% end %>

<div class="container">
  <div class="row">
    <h1>Bookings</h1>
    <% counter = 1 %>
    <% @bookings.each do |booking| %>
      <p> <%= counter %> | <%= booking.start_date %> | <%= booking.end_date %> | <%= link_to "edit" %> | <%= link_to "create" %> | <%= link_to "delete" %></p>
      <% counter += 1 %>
    <% end %>
  </div>
</div>
